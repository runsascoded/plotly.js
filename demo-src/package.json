{
  "name": "zorder-pan-bug-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:before": "VITE_PLOTLY_SRC=cdn vite build --base=/plotly.js/before/ --outDir=../before --emptyOutDir",
    "build:after": "VITE_PLOTLY_SRC=local vite build --base=/plotly.js/after/ --outDir=../after --emptyOutDir"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-plotly.js": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "~5.6.2",
    "vite": "^6.0.1"
  }
}
